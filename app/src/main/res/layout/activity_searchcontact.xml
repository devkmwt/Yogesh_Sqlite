<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.yogesh.sqlite.Searchcontact">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="MOBILE NUMBER"
        android:textSize="20dp"
        android:id="@+id/textView4" />
     <EditText
         android:id="@+id/mobilenum"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:layout_below="@+id/textView4"
         android:layout_alignParentStart="true" />
    <Button
        android:id="@+id/searchcon"
        android:text="search"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/mobilenum"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/showName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/searchcon"
        android:layout_marginStart="128dp"
        android:layout_marginTop="110dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/showemail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_below="@+id/showName"
        android:layout_alignStart="@+id/showName"
        android:layout_marginTop="76dp" />
</RelativeLayout>
